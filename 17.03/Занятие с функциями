def f (n):
    print("Привет",n)
f('Егор')
f('Полина')
f('Маша')
f(input())


def print_fio (n,q,e):
    print(n[0],q[0],e[0], sep ="")
d = str(input())
a = str (input())
s = str(input())
print_fio(d,a,s)

